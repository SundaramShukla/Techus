
{
    "version": 2,
    "builds": [{
        "src": "Ritkatech/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distdir":"staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "/Recharge/static/(.*)",
            "dest": "/Recharge/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Ritkatech/wsgi.py"
        }
    ]
}